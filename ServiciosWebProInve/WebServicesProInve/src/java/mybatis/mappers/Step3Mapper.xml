<mapper namespace="Step3">        
    <insert id="guardarStep3" parameterType="pojos.Step3">
        INSERT INTO ProInver.dbo.Step1 (id_direccion, calle, numero, id_codigo_postal, id_step1) 
        VALUES(#{id_direccion}, #{calle}, #{numero}, #{id_codigo_postal}, #{id_step1})
    </insert>    
    
    <insert id="proAlmGuardarStep3" statementType="CALLABLE" parameterType="pojos.Step3">
        call SPIStep3(#{}, #{}, #{}, #{}, #{}, #{})
    </insert>
    
</mapper>
